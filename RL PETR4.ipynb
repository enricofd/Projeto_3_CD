    "from datetime import datetime\n",
    "str_date = '06.11.2019'\n",
    "def padronizaDia(str_date):\n",
    "    date = datetime.strptime(str_date, '%d.%m.%Y').date()\n",
    "    if len(str(date.day))==1:\n",
    "        dia = \"0\" + str(date.day)\n",
    "    else:\n",
    "        dia = str(date.day)\n",
    "        \n",
    "    if len(str(date.month))==1:\n",
    "        mes = \"0\" + str(date.month)\n",
    "    else:\n",
    "        mes = str(date.month)\n",
    "    return \"{0}-{1}-{2}\".format(date.year,mes, dia)"
   ]
